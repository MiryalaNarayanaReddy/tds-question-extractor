{
  "manifest_version": 3,
  "name": "Exam Question Extractor",
  "version": "1.0",
  "description": "Extract questions from exam page in logged-in browser",
  "permissions": ["activeTab", "scripting", "storage", "downloads"],
  "host_permissions": ["https://exam.sanand.workers.dev/*"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://exam.sanand.workers.dev/*"],
      "js": ["content.js"]
    }
  ]
}
